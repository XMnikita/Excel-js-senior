{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"],
    "object-curly-spacing": "off",
    "indent": "off",
    "operator-linebreak": "off",
    "spaced-comment": "off",
    "space-before-function-paren": "off",
    "no-debugger": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
